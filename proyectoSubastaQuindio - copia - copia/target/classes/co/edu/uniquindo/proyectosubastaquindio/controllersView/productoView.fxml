<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="co.edu.uniquindo.proyectosubastaquindio.controllersView.ControllerProductoView"
            prefHeight="400.0" prefWidth="600.0">

                        <AnchorPane prefHeight="569.0" prefWidth="866.0">
                            <children>
                                <Pane fx:id="panePrincipal" layoutX="5.0" layoutY="7.0" prefHeight="563.0"
