                                              stylesheets="@../css/prodcutoCss.css">
                                            <children>
                                                <TableView fx:id="tableDatos" layoutX="24.0" layoutY="15.0"
                                                           prefHeight="183.0" prefWidth="782.0"
                                                           stylesheets="@../css/prodcutoCss.css">
                                                    <columns>
                                                        <TableColumn fx:id="columnaUno" prefWidth="160.0"
                                                                     text="nombre Producto"/>
                                                        <TableColumn fx:id="columnaDos" prefWidth="180.0"
                                                                     text="descripcion"/>
                                                        <TableColumn fx:id="columnaTres" prefWidth="236.0"
