                                                            </HBox.margin>
                                                        </Button>
                                                        <Button fx:id="btnActualizar" mnemonicParsing="false"
                                                                onAction="#Actualizar" prefHeight="42.0"
                                                                prefWidth="105.0" text="Actualizar">
                                                            <HBox.margin>
                                                                <Insets right="170.0"/>
                                                            </HBox.margin>
                                                        </Button>
                                                        <Button fx:id="btnEliminar" alignment="CENTER"
                                                                contentDisplay="CENTER" mnemonicParsing="false"
                                                                onAction="#eliminar" prefHeight="42.0" prefWidth="105.0"
